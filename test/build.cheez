std :: import "std.cheez"
io :: std.IO

_ :: Main()
Main :: () {
    f := Foo(123, "123")
    // Foo.print(^f)
    fa := &f

    io.print_string(fa.a)
    io.print_string((&f).a)
    io.print_string(f.a)
    io.print_string(Foo(456, "456").a)

}

Foo :: struct {
    uiae : int
    a : string
}

impl Foo {
    print :: (self: ^Foo) {
        io.print_string(self.a)
    }
}

