std :: import "std.cheez"
io :: std.IO

_ :: Main()
Main :: () {
    mut foo := Foo(5)
    Foo.a()
    foo.b()
    foo.c()
    foo.d()
}

Foo :: struct {
    v : int
}

impl Foo {
    pub a :: () do io.print_string("a()")

    pub b :: (mut self: Foo) do
        io.print_string("b()")
        io.print_int(self.v)
        self.v = self.v + 1

    pub c :: (self: &Foo) do
        io.print_string("c()")
        io.print_int(self.v)
        // self.v = self.v + 1

    pub d :: (self: &mut Foo) do
        io.print_string("d()")
        io.print_int(self.v)
        self.v = self.v + 1
}