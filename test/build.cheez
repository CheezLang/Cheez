std :: import "std.cheez"

_ :: Main()
Main :: () {
    x := Foo.foo("lol")
    io :: std.IO
    io.print_string(x)
    io.print_string(Foo.NAME)
}

Foo :: struct {
    a : int
}

impl Foo {
    NAME :: "Foo"
    ArgType :: string
    ReturnType :: string

    foo :: (str: ArgType) -> ReturnType {
        std.IO.print_string("Foo.foo()")
        std.IO.print_string(str)
        std.IO.print_bool(Foo.Test)
        return "aaa"
    }
}

impl Foo {
    Test :: false
}

